{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "build": "vite build",
    "preview": "vite preview",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=__tests__/unit --passWithNoTests",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=__tests__/integration --runInBand --passWithNoTests",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=__tests__/e2e --runInBand --passWithNoTests",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --runInBand"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "papaparse": "^5.4.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "babel-jest": "^29.7.0"
  }
}